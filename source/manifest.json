{
	"manifest_version": 2,
	"name": "Feedly Counter",
	"description": "Shows unread article count from Feedly",
	"version": "1.1.3",
	"options_page": "options.html",
	"icons": {
		"128": "/icons/icon128.png",
		"48": "/icons/icon48.png"
	},
	"permissions": [
		"tabs", "webRequest", "alarms", "http://cloud.feedly.com/*"
	],
	"browser_action": {
		"default_icon": "/icons/icon_disabled.png"
	},
	"background": {
		"persistent": true,
		"scripts": [
			"background.js"
		]
	}
}